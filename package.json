{
  "version": "0.4.0",
  "name": "@tunnckocore/template",
  "description": "WIP: some description for your template",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com>",
  "repository": "tunnckocorehq/template",
  "homepage": "https://github.com/tunnckocorehq/template",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "yarn scripts devest lint",
    "test": "yarn scripts devest test",
    "build": "yarn scripts pika-pack build --out dist",
    "status:pass": "badgen --subject build --status passing --color green > dist/badge.svg",
    "status:fail": "badgen --subject build --status failing --color red > dist/badge.svg",
    "pkg": "yarn lint && yarn test && yarn build",
    "now-build": "(yarn pkg && yarn status:pass) || yarn status:fail"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": "**/__tests__/**"
        }
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "keywords": [
    "tunnckocorehq",
    "template",
    "tunnckocore-template"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@pika/pack": "^0.4.0",
    "@pika/plugin-build-node": "^0.4.0",
    "@pika/plugin-build-web": "^0.4.0",
    "@pika/plugin-standard-pkg": "^0.4.0",
    "@tunnckocore/config": "^4.0.1",
    "@tunnckocore/devest": "^0.6.1",
    "@tunnckocore/scripts": "^1.3.0",
    "@tunnckocore/scripts-config": "^4.0.2",
    "@types/jest": "^24.0.15",
    "badgen-cli": "^0.1.1",
    "jest": "^24.8.0",
    "jest-runner-eslint": "^0.7.4"
  }
}
